#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i,j,n,k,sum=0;
    printf("enter the values:\n");
    scanf("%d%d",&n,&k);
    for(i=1;i<=n;i++)
    {
        printf("%d\n",n);
    }
    for(j=1;j<=k;j++)
    {
        sum =sum+j;
    }
    printf("the sum is :%d\n",sum);
    return 0;
}
